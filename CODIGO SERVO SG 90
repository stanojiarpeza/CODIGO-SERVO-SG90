#include <Servo.h>

Servo motor1;
Servo motor2;
Servo motor3;
Servo motor4;
int posicion =0;

void setup() {
  Serial.begin(9600);
  motor1.attach(2);
  motor2.attach(3);
  motor3.attach(4);
  motor4.attach(5);
  // put your setup code here, to run once:

}

void loop() {
  motor1.write(90);
  delay(1000);
  motor2.write(90);
  delay(1000);
  motor3.write(90);
  delay(1000);
  motor4.write(90);
  delay(1000);
  // put your main code here, to run repeatedly:

}
